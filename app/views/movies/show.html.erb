<h1>View Movie's <%= pluralize(@actors.count, "Actor") %></h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th scope="col">First Name</th>
			<th scope="col">Last Name</th>
		</tr>
	</thead>
	<tbody>
		<% @actors.each do |actor| %>
		<tr>
			<td><%= actor.first_name %></td>
			<td><%= actor.last_name %></td>
		</tr>
		<% end %>
	</tbody>
</table>
